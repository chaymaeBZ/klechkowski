<% provide(:title, 'Sign up') %>
<div class="new">
<div class="ui middle aligned center aligned grid" >
  <div class="column">
    <h2 class="ui image header" >
    <div class="column"><img src="<%= asset_path 'wilaya.png' %>" class="image" height="110" width="110" style="margin: 0 0 10px 25px;"></div>
      
      <div class="white" >
        خاص بموظفي الولاية
      </div>
    </h2>
</div>
</div>


<div class="ui raised segment" style="margin: 60px 400px; background: rgb(246, 227, 186);">


  <h4 class="ui dividing large header">Shipping Information</h4>
<%= render 'shared/flash_message' %>
    <%= form_for(:session, url: login_path, html: { class: 'ui form' }) do |f| %>



    <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email, placeholder: "foobar@example.com", type: 'email'%>
    </div>


    <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password, placeholder: "Password", type: 'password' %>
    </div>

    <div class="field">
    <div class="ui checkbox">
	<%= f.check_box :remember_me, class: "hidden" %>
    <%= f.label :remember_me, "Remember me" %>
	</div>
	</div>

    <%= f.submit "Log in", class: "ui fluid blue button" %>
    <% end %>



</div>
</div>